<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="CompositeApp1" xmlns:ns2="http://proto/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
    <service-assembly>
        <identification>
            <name>CompositeApp1</name>
            <description>Represents the Service Assembly of CompositeApp1</description>
        </identification>
        <service-unit>
            <identification>
                <name>CompositeApp1-sun-http-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-http-binding.jar</artifacts-zip>
                <component-name>sun-http-binding</component-name>
            </target>
        </service-unit>
        <connections>
            <connection>
                <consumer endpoint-name="casaPort1" service-name="ns1:CompositeApp1Service1"/>
                <provider endpoint-name="ProtoWebServicePort" service-name="ns2:ProtoWebServiceService"/>
            </connection>
        </connections>
    </service-assembly>
</jbi>
